Sys_AudioEntity = { Id = 0, AssetPath = "", Volume = 0, IsLoop = 0, Priority = 0 }

Sys_AudioEntity.__index = Sys_AudioEntity;

function Sys_AudioEntity.New(Id, AssetPath, Volume, IsLoop, Priority)
    local self = { };
    setmetatable(self, Sys_AudioEntity);

    self.Id = Id;
    self.AssetPath = AssetPath;
    self.Volume = Volume;
    self.IsLoop = IsLoop;
    self.Priority = Priority;

    return self;
end