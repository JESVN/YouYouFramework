Sys_SceneEntity = { Id = 0, SceneName = "", BGMId = 0, SceneType = 0, Content_En = "", Content_Chn = "" }

Sys_SceneEntity.__index = Sys_SceneEntity;

function Sys_SceneEntity.New(Id, SceneName, BGMId, SceneType, Content_En, Content_Chn)
    local self = { };
    setmetatable(self, Sys_SceneEntity);

    self.Id = Id;
    self.SceneName = SceneName;
    self.BGMId = BGMId;
    self.SceneType = SceneType;
    self.Content_En = Content_En;
    self.Content_Chn = Content_Chn;

    return self;
end